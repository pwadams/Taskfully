  <div class="container">
  <div class="page-header">
    <h1>Users <%=link_to "New User", new_user_path, class: "btn btn-info pull-right" %></h1>
  </div>
  <table class="table">
    <thead>
      <th>Name</th>
      <th>Email</th>
    </thead>

    <tbody>
          <tr>
          <% @users.each do |user| %>
            <td>
              <%= link_to "#{user.first_name} #{user.last_name}", user %>
            </td>
            <td>
              <%= mail_to user.email %>
            </td>
            <td>
              <%= link_to "Edit", edit_user_path(user) %>
            </td>
          </tr>
        <% end %>
    </tbody>
  </table>
  </div>
