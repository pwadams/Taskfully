<div class= "container">
    <div class="pull-right">
    <%=link_to "Edit", edit_task_path(@task), class: "btn btn-info" %>
    </div>

    <div>
      <h1>
      <% if @task.is_complete %>
       <s><%=@task.description %></s>
      <% else %>
      <%=@task.description %>
      <% end %>
      </h1>
    </div>

    <ol class="breadcrumb">
      <li><%= link_to "Tasks", tasks_path %></li>
      <li><%= @task.description %></li>
    </ol>

    <h4>
      Due Date:
      <% if @task.due_date %>
        <%= @task.due_date.strftime("%m/%d/%Y") %>
      <% else %>
      <% end %>
    <h4>
</div>
