<div class= "container">
  <div class="page-header">
    <h1>New Task</h1>
  </div>

<%= form_for @task do |f| %>
<div>
  <%= f.label :name  %>
  <%= f.text_field :name %>
</div>

<div>
  <%= f.label :description %>
  <%= f.text_field :description %>
</div>

<%= f.submit "Create Task" %>
<div><%=link_to "Back", tasks_path %> </div>
<% end %>
