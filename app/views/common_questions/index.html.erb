<div class= "page-header">
  <h1>Frequently Asked Questions</h1>
</div>

<ul>
<% @faq_array.each do |faq| %>
    <li><%= link_to faq.question, "##{faq.slug}" %></li>
<% end %>
</ul>


<% @faq_array.each do |faq| %>
    <h2 id="<%= faq.slug %>"><%= faq.question %></h2>
    <p><%= faq.answer %></p>
<% end %>
