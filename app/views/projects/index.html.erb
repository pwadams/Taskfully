<div class="page-header">
  <h1>Projects <%=link_to "New Project", new_project_path, class: "btn btn-info pull-right" %></h1>
</div>

<table class="table">
  <thead>
    <th>Name</th>
  </thead>
  <tbody>
      <% @projects.tasks.each do |tasks| %>
        <tr>
          <td><%= link_to task.count, project_path(@task) %></td>
        </tr>
      <% end %>
  </tbody>
</table>
</div>

<ul>
<% @cat.toys.each do |toy| %>
  <li><%= toy.name %></li>
  <% end %>
</ul>

</div>
